    "@microsoft/fast-foundation": "^2.47.1-0",
    "@microsoft/fast-web-utilities": "^6.0.0",
    "@n8d/htwoo-core": "^1.3.0",
    "@playwright/test": "^1.31.2",
    "@types/copyfiles": "^2.4.1",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^29.4.4",
    "@types/node": "^18.15.3",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
